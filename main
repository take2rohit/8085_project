	LXI SP, 400FH
	LDA 7001H
	LXI H, 7000H
	SUB M
	ADI 01H
 	MOV D, A
	MOV E, A
MAIN:	LXI H, 7000H
	MOV B, M
	CALL MUL
 	INX H
	INX H
	CALL MUL
	PUSH B
	LDA 7003H
	MOV B, A
	DCX H
	DCX H
	CALL MUL
	PUSH B
	LDA 7004H
	POP B
	ADD B
	POP B
	ADD B
	MOV B, A
	PUSH B
	LXI H, 7000H
	INR M
	DCR D
	JNZ MAIN
	LXI H, 7005H
ST:	POP B
	MOV M, B
	INX H
	DCR E
	JNZ ST
	HLT
MUL:	XRA A
LOOP:	ADD M
	DCR B
	JNZ LOOP
	MOV B,A
	RET